{
    "workbench.colorTheme": "Moonlight II Italic",
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": true,
    "window.zoomLevel": 0,
    // "editor.formatOnSave": true,
    "workbench.colorCustomizations": {
        /* "[Palenight Operator]": {
            "sideBar.background": "#111",
            "editor.background": "#161616",
            "editor.lineHighlightBackground": "#222",
            "tab.activeBackground": "#252525",
            "tab.inactiveBackground": "#1E1E1E",
            "activityBar.background": "#0D0D0D",
            "statusBar.background": "#111",
            "editorGroupHeader.tabsBackground": "#252525",
            "scrollbarSlider.background": "#252525",
            "scrollbarSlider.hoverBackground": "#444"
        } */
    },
    "editor.tabSize": 2,
    "editor.renderWhitespace": "none",
    "editor.fontFamily": "'Dank Mono'",
    // "editor.fontFamily": "'Operator Mono'",
    // , 'Fira Code', Menlo, Monaco, 'Courier New', monospace"
    "editor.fontLigatures": true,
    "editor.fontWeight": "400",
    "terminal.integrated.fontFamily": "Iosevka Term",
    "terminal.integrated.fontSize": 14,
    "editor.rulers": [
        140
    ],
    "workbench.iconTheme": "material-icon-theme",
    "workbench.statusBar.feedback.visible": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.visible": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
        // "editor.defaultFormatter": "mblode.pretty-formatter"
        // "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.fontSize": 16,
    "terminal.integrated.rendererType": "dom",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
